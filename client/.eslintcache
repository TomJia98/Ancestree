ptions = {\r\n    layout: {\r\n      hierarchical: {\r\n        direction: \"UD\",\r\n        enabled: true,\r\n        sortMethod: \"directed\",\r\n        shakeTowards: \"leaves\",\r\n      },\r\n    },\r\n    edges: {\r\n      color: \"#000000\",\r\n    },\r\n    height: \"500px\",\r\n  };\r\n\r\n  const events = {\r\n    //add selection criteria to not switch if clicking away from a node\r\n    select: function (event) {\r\n      var { nodes, edges } = event;\r\n      if (nodes[0] === personId) {\r\n        setSelectedNode();\r\n      } else setSelectedNode(nodes[0]);\r\n    },\r\n  };\r\n\r\n  return (\r\n    <main>\r\n      {Auth.loggedIn() ? (\r\n        <>\r\n          <SinglePersonInfo current={selectedNode} />\r\n          {isGraphFinished ? (\r\n            <>\r\n              <Graph\r\n                key={graphKey}\r\n                graph={graph}\r\n                options={options}\r\n                events={events}\r\n              />\r\n            </>\r\n          ) : (\r\n            <>\r\n              <p>family tree is being rendered</p>\r\n            </>\r\n          )}\r\n        </>\r\n      ) : (\r\n        <>\r\n          <p>\r\n            It looks like you are not logged in! please log in or signup\r\n            <Link to=\"/login\">here</Link>\r\n          </p>\r\n        </>\r\n      )}\r\n    </main>\r\n  );\r\n};\r\n\r\nexport default Main;\r\n","C:\\Users\\Pmmmm\\OneDrive\\Desktop\\uwa code\\project 3\\Ancestree\\Ancestree\\client\\src\\components\\SinglePersonInfo.js",["100","101","102","103","104"],"C:\\Users\\Pmmmm\\OneDrive\\Desktop\\uwa code\\project 3\\Ancestree\\Ancestree\\client\\src\\components\\addChild.js",["105","106","107","108","109","110","111","112","113","114","115","116"],"import React, { useState, Component, useEffect } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { useQuery, useMutation } from \"@apollo/client\";\r\nimport {\r\n  QUERY_SINGLE_PERSON,\r\n  QUERY_PERSONS,\r\n  QUERY_PERSO